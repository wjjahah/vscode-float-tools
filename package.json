{
  "name": "float-tools",
  "displayName": "float-tools",
  "description": "一个vscode插件，用于浮点数相关的各种格式之间的转换",
  "icon": "logo/logo.ico",
  "version": "0.0.9",
  "publisher": "wjjahah",
  "repository": {
    "type": "git",
    "url": "https://github.com/wjjahah/vscode-float-tools.git"
  },
  "engines": {
    "vscode": "^1.50.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "keywords": [
    "float",
    "tools",
    "formart",
    "convert"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "float-tools.convertSelectNum",
        "title": "float-tools.convertSelectNum"
      }
    ],
    "languages": [
      {
        "id": "c",
        "aliases": [
          "c",
          "C"
        ],
        "extensions": [
          ".c"
        ],
        "configuration": "./language-configuration.json"
      },
      {
        "id": "cpp",
        "aliases": [
          "Cpp",
          "CPP"
        ],
        "extensions": [
          ".c",
          ".cpp",
          ".maca",
          ".cu"
        ],
        "configuration": "./language-configuration.json"
      },
      {
        "id": "asm",
        "aliases": [
          "s",
          "ASM"
        ],
        "extensions": [
          ".s",
          ".S",
          ".asm",
          ".ASM"
        ],
        "configuration": "./language-configuration.json"
      },
      {
        "id": "others",
        "aliases": ["Others"],
        "extensions": [
          ".log",
          ".ll",
          ".txt",
          ".ptx",
          ".ii"
        ],
        "configuration": "./language-configuration.json"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.50.0",
    "@types/mocha": "^10.0.6",
    "@types/node": "14.x",
    "@typescript-eslint/eslint-plugin": "^7.11.0",
    "@typescript-eslint/parser": "^7.11.0",
    "eslint": "^8.57.0",
    "typescript": "^5.4.5",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.4.0"
  }
}
